
class Node:
    def __init__(self,data=None,next=None):
        self.data=data
        self.next=next

class Queue:
    def __init__(self):
        self.head=None
        self.front=None
        self.rear=None

    def push(self,val):
        obj=Node(val)
        if self.head is None:
            self.head=obj
            self.front=obj
            self.rear=obj
            print("pushed!")
            return
        self.rear.next=obj
        self.rear=obj
        print("Pushed!")

    def display(self):
        if self.head is None:
            print("nooo")
            return
        itr=self.front
        while itr:
            print(itr.data,end="->")
            itr=itr.next
        print()
        return

    def pop(self):
        if self.head is None:
            print("UnderFitting!")
            return
        itr=self.front
        if self.front is self.rear:
            self.head=None
            self.front=None
            self.rear=None
            print("poped!")
            return
        self.front=self.front.next
        self.head=self.front
        print("poped!")
        return
